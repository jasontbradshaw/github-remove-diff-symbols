{
  "name": "Remove Github Diff Symbols",
  "version": "0.0.0",
  "manifest_version": 2,

  "description": "Removes the +/- symbols from diff lines.",

  "content_scripts": [
    {
      "matches": [ "*://*.github.com/*" ],
      "js": [ "main.js" ],
      "run_at": "document_idle",
      "all_frames": true
    }
  ]
}
